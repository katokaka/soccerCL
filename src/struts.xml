<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.ui.theme" value="simple" />
	<package name="com.internousdev.soccerCL.action" extends="struts-default">

		
	<!-- 加藤記載 -->
		<!-- 商品詳細画面へと遷移するためのアクション -->
		<action name="GoMatchDetailAction" class="com.internousdev.soccerCL.action.GoMatchDetailAction"
		method="execute">
		<result name="success">match_detail.jsp</result>
		<result name="error">Index.jsp</result>
		</action>
		

	</package>
</struts>

